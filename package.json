{
  "name": "mongodb-backup-cloud",
  "version": "1.0.7",
  "description": "backup mongodb databases and upload to github/google drive",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://github.com/ShamalRathnayake/Mongodb-backup-cloud",
  "files": [
    "build/**"
  ],
  "scripts": {
    "dev": "nodemon --config nodemon.json src/index.ts",
    "start": "tsc && node build/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dryrun": "npm publish --dry-run"
  },
  "keywords": [
    "mongodb",
    "backup",
    "google drive",
    "cron",
    "git",
    "mongo"
  ],
  "author": "ShamalRathnayake",
  "license": "ISC",
  "devDependencies": {
    "@octokit/types": "^6.34.0",
    "@types/jest": "^29.5.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.6.2",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/rest": "^18.12.0",
    "@types/node-cron": "^3.0.1",
    "globby": "^11.1.0",
    "googleapis": "^126.0.0",
    "mime-types": "^2.1.34",
    "node-cron": "^3.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
